{
  "name": "feynman",
  "productName": "Feynman Library Manager",
  "version": "1.0.0",
  "description": "Comprehensive library managment tool for Feynman High School",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --allow-file-access-from-files --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . 'Feynman Library Manager' --allow-file-access-from-files --overwrite --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Feynman Library Manager\""
  },
  "author": "Varun Shenoy",
  "license": "MIT",
  "dependencies": {
    "electron-debug": "^1.4.0",
    "jquery": "^3.2.1",
    "jspdf": "^1.3.5",
    "jspdf-autotable": "^2.3.2",
    "moment": "^2.20.1",
    "node-json-db": "^0.7.3",
    "sweetalert2": "^7.1.2"
  },
  "devDependencies": {
    "electron": "^1.7.10",
    "electron-packager": "^10.1.0",
    "electron-winstaller": "^2.6.4"
  }
}
